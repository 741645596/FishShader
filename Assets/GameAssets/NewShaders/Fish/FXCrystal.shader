				float2 uv_FlowTex = IN.ase_texcoord3.xyz.xy * _FlowTex_ST.xy + _FlowTex_ST.zw;
				float temp_output_73_0 = ( ( _TimeParameters.x * 0.05 ) * _FlowSpeed );
				float2 appendResult78 = (float2(uv_FlowTex.x , ( uv_FlowTex.y + temp_output_73_0 )));
				float2 appendResult77 = (float2(( uv_FlowTex.x + temp_output_73_0 ) , uv_FlowTex.y));
				#if defined(_FLOWDIRECTION_X)
