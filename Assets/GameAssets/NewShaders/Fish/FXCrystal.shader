				#elif defined(_FLOWDIRECTION_Y)
				float2 staticSwitch79 = appendResult78;
				#else
				float2 staticSwitch79 = appendResult78;
				#endif
				float2 uv_MainTex = IN.ase_texcoord3.xyz.xy * _MainTex_ST.xy + _MainTex_ST.zw;
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float3 ase_worldReflection = reflect(-ase_worldViewDir, ase_worldNormal);
				// float4 temp_output_40_0 = ( ( tex2D( _MainTex, uv_MainTex ) * _MainColor ) * texCUBE( _RefractText, ase_worldReflection ) * _RefractInt );
				float4 temp_output_40_0 = ( ( tex2D( _MainTex, uv_MainTex ) * _MainColor ) * texCUBE( _RefractText, ase_worldReflection ) * (_RefractInt + 2) );
				
				float3 Color = ( ( tex2D( _FlowTex, staticSwitch79 ) * _FlowInt ) + temp_output_40_0 ).rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;

